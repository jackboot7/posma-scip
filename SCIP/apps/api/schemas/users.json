        "properties": {
            "average_hours_worked": {
                "type": "number",
                "required": false
            },
            "email": {
                "type":"string",
                "required": false
            },
            "first_name": {
                "type": "string",
                "required": false
            },
            "is_working": {
                "type": "boolean",
                "required": false
            },
            "last_name": {
                "type": "string",
                "required": false
            },
            "last_workday": {
                "type": "object",
